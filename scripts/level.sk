command /reset:
	permission: status_reset.admin
	permission message: Unknow command. Type "/help" for help.
	trigger:
		set {%player's uuid%.skRPG_level} to 1
		set {%player's uuid%.skRPG_exp} to 0
		set {%player's uuid%.skRPG_next_exp} to 16
		set {%player's uuid%.skRPG_hp} to 10
		set {%player's uuid%.skRPG_str} to 1
		set {%player's uuid%.skRPG_luck} to 0.1
		set {%player's uuid%.skRPG_star} to 0
		set {%player's uuid%.skRPG_level_max} to false
		set {%player's uuid%.skRPG_hp_level} to 0
		set {%player's uuid%.skRPG_str_level} to 0
		set {%player's uuid%.skRPG_luck_level} to 0
		set {%player's uuid%.skRPG_syn} to 0
		set {%player's uuid%.skRPG_exp_level} to 0
		set {%player's uuid%.skRPG_money_level} to 0
		set max health of player to ({%player's uuid%.skRPG_hp})
		player.setHealthScale(20.0)
		send "リセット完了"
on join:
	while player is online:
		set the player's tablist name to " &e&l✯%{%player's uuid%.skRPG_star}%&f&l | %{%player's uuid%.syougou}%&7 | &f&l%player% &b&lLv.&f&l%{%player's uuid%.skRPG_level}%"
		wait 1 second



on join:

	if {%player's uuid%.skRPG_level} is not set:
		set {%player's uuid%.skRPG_level} to 1
	if {%player's uuid%.skRPG_exp} is not set:
		set {%player's uuid%.skRPG_exp} to 0
	if {%player's uuid%.skRPG_next_exp} is not set:
		set {%player's uuid%.skRPG_next_exp} to 16
	if {%player's uuid%.skRPG_hp} is not set:
		set {%player's uuid%.skRPG_hp} to 10
	if {%player's uuid%.skRPG_str} is not set:
		set {%player's uuid%.skRPG_str} to 1
	if {%player's uuid%.skRPG_luck} is not set:
		set {%player's uuid%.skRPG_luck} to 0.1	
	if {%player's uuid%.skRPG_star} is not set:
		set {%player's uuid%.skRPG_star} to 0
	if {%player's uuid%.skRPG_level_max} is not set:
		set {%player's uuid%.skRPG_level_max} to false
	if {%player's uuid%.skRPG_hp_level} is not set:
		set {%player's uuid%.skRPG_hp_level} to 0
	if {%player's uuid%.skRPG_str_level} is not set:
		set {%player's uuid%.skRPG_str_level} to 0
	if {%player's uuid%.skRPG_luck_level} is not set:
		set {%player's uuid%.skRPG_luck_level} to 0
	if {%player's uuid%.skRPG_syn} is not set:
		set {%player's uuid%.skRPG_syn} to 0
	if {%player's uuid%.skRPG_exp_level} is not set:
		set {%player's uuid%.skRPG_exp_level} to 0
	if {%player's uuid%.skRPG_money_level} is not set:
		set {%player's uuid%.skRPG_money_level} to 0
	if {%player's uuid%.skRPG_money} is not set:
		set {%player's uuid%.skRPG_money} to 100
	
	set max health of player to ({%player's uuid%.skRPG_hp})
	player.setHealthScale(20.0)
on join:
	if {%player's uuid%.syougou} is not set:
		set {%player's uuid%.syougou} to "&6&lPlayer"


