on death:
	if {%attacker's uuid%.skRPG_level} is less than 100:
		if {%attacker's uuid%.skRPG_exp} is more than ({%attacker's uuid%.skRPG_next_exp} - 1):
			add 1 to {%attacker's uuid%.skRPG_level}
			if {%attacker's uuid%.skRPG_level_max} is false:
				if {%attacker's uuid%.skRPG_level} is 100:
					set {%attacker's uuid%.skRPG_level_max} to true				
					send "&eLevel 100!" to attacker
					send attacker title "&e&l Level up" with subtitle "&b&lLv.100" for 5 seconds
					add (0.3 * {%attacker's uuid%.skRPG_level}) + (0.1 * {%attacker's uuid%.skRPG_hp_level}) + (0.1 * {%attacker's uuid%.skRPG_syn}) to {%attacker's uuid%.skRPG_hp}
					add (0.03 * {%attacker's uuid%.skRPG_level}) + (0.1 * {%attacker's uuid%.skRPG_str_level}) + (0.1 * {%attacker's uuid%.skRPG_syn}) to {%attacker's uuid%.skRPG_str}
					add 0.1 + (0.1 * {%attacker's uuid%.skRPG_luck_level}) + (0.1 * {%attacker's uuid%.skRPG_syn}) to {%attacker's uuid%.skRPG_luck}
					play "ENTITY_PLAYER_LEVELUP" to attacker at volume 5
					play "ENTITY_FIREWORK_TWINKLE" to attacker at volume 5
					set max health of attacker to ({%attacker's uuid%.skRPG_hp})
					attacker.setHealthScale(20.0)
					stop
				else:
					message "&b&lLevel %{%attacker's uuid%.skRPG_level} - 1% &d&l→ &b&lLevel %{%attacker's uuid%.skRPG_level}% " to attacker
					send attacker title "&e&l Level up" with subtitle "&b&lLevel %{%attacker's uuid%.skRPG_level} - 1% &d&l→ &b&lLevel %{%attacker's uuid%.skRPG_level}%" for 5 seconds
					play "ENTITY_PLAYER_LEVELUP" to attacker at volume 5
					set {%attacker's uuid%.skRPG_next_exp} to {%attacker's uuid%.skRPG_level} * 52
					set {%attacker's uuid%.skRPG_exp} to 0
					add (0.3 * {%attacker's uuid%.skRPG_level}) + (0.1 * {%attacker's uuid%.skRPG_hp_level}) + (0.1 * {%attacker's uuid%.skRPG_syn}) to {%attacker's uuid%.skRPG_hp}
					add (0.03 * {%attacker's uuid%.skRPG_level}) + (0.1 * {%attacker's uuid%.skRPG_str_level}) + (0.1 * {%attacker's uuid%.skRPG_syn}) to {%attacker's uuid%.skRPG_str}
					add 0.1 + (0.1 * {%attacker's uuid%.skRPG_luck_level}) + (0.1 * {%attacker's uuid%.skRPG_syn}) to {%attacker's uuid%.skRPG_luck}
					set max health of attacker to ({%attacker's uuid%.skRPG_hp})
					attacker.setHealthScale(20.0)
					send "&e[✯%{%attacker's uuid%.skRPG_star}%] &f%attacker%のステータス [Lv.%{%attacker's uuid%.skRPG_level}%]" to attacker
					send "体力: %{%attacker's uuid%.skRPG_hp}%" to attacker
					send "攻撃力: %{%attacker's uuid%.skRPG_str}%" to attacker
					send "運: %{%attacker's uuid%.skRPG_luck}%" to attacker
					send "次のレベルに必要なEXP量: %{%attacker's uuid%.skRPG_next_exp}%" to attacker
					stop


	




