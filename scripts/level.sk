every 1 seconds:
	loop all players:
		#アクションバーの変更をしろ
		#ーーーーーーーーーーーーーーーここーーーーーーーーー
		show an action bar from "&f&lLv&8&l.&f&l%{%loop-player's uuid%.skRPG_level}% &c&lHP &f&l%health of loop-player% &f&l/ &f&l%{%loop-player's uuid%.skRPG_hp}% &b&lEXP &f&l%{%loop-player's uuid%.skRPG_exp}% &f&l/ &f&l%{%loop-player's uuid%.skRPG_next_exp}%" to loop-player
		#ーーーーーーーーーーーーーーーまでーーーーーーーーー

on chat:
	set {_prefix} to {player::prefix::%player's uuid%}
	if {_prefix} is not set:
		set {_prefix} to "%{%player's uuid%.syougou}%"
	set the chat format to "&e&l✯%{%player's uuid%.skRPG_star}% &8&l| &7%{_prefix}% &8&l| &f&l%player%&8&l : &f&l%message%"
	
	
	#ここのステータスコマンドはそのままにしたかったらそのままでいいけど
	#もし変更したかったら
	# if ぶんにはLv.100になったときだから注意決してEXPは間違ってないいいね？
command /status:
	trigger:
		send "&e[✯%{%player's uuid%.skRPG_star}%] &f%player%のステータス [Lv.%{%player's uuid%.skRPG_level}%]"
		send "&e所持Gold&7: &e%{%player's uuid%.skRPG_money}% "
		if {%player's uuid%.skRPG_level} is 100:
			send "&bEXP&7: &b2028 &7/ &b2028"
		else:
			send "&bEXP&7: &b%{%player's uuid%.skRPG_exp}% &7/ &b%{%player's uuid%.skRPG_next_exp}%"
		send "体力: %{%player's uuid%.skRPG_hp}%"
		send "攻撃力: %{%player's uuid%.skRPG_str}%"
		send "運: %{%player's uuid%.skRPG_luck}%"
		stop



		
		


		
		
